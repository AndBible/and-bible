{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "type-check:ci": "vue-tsc --noEmit -p tsconfig.json --composite false > type-check-results.txt",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "build-debug": "cross-env NODE_ENV=debug vite build --mode debug",
    "build-production": "cross-env NODE_ENV=production vite build --mode production",
    "build-development": "cross-env NODE_ENV=development vite build --mode development",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color > lint-results.txt",
    "lint:silent": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color",
    "test:ci": "vitest --root src/ --run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~6.2.1",
    "@fortawesome/free-solid-svg-icons": "~6.2.1",
    "@fortawesome/vue-fontawesome": "~3.0.0-5",
    "@vue/runtime-core": "~3.2.45",
    "color": "~4.2.3",
    "lodash": "~4.17.21",
    "mitt": "~3.0.0",
    "sprintf-js": "~1.1.2",
    "vue": "~3.2.45",
    "vuedraggable": "~4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.2.0",
    "@types/color": "~3.0.3",
    "@types/jest": "~27.0.1",
    "@types/js-yaml": "~4.0.5",
    "@types/jsdom": "~20.0.1",
    "@types/lodash": "~4.14.190",
    "@types/node": "~18.11.11",
    "@types/sprintf-js": "~1.1.2",
    "@typescript-eslint/eslint-plugin": "~5.45.1",
    "@typescript-eslint/parser": "~5.45.1",
    "@vitejs/plugin-vue": "~3.2.0",
    "@vue/compiler-sfc": "~3.2.45",
    "@vue/eslint-config-typescript": "~11.0.2",
    "@vue/reactivity": "~3.2.45",
    "@vue/test-utils": "~2.2.6",
    "@vue/tsconfig": "~0.1.3",
    "@vue/vue3-jest": "~27.0.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.29.0",
    "eslint-plugin-vue": "~9.8.0",
    "jest-raw-loader": "~1.0.1",
    "js-yaml": "~4.1.0",
    "jsdom": "18.0.0",
    "json-loader": "~0.5.7",
    "npm": "~9.1.2",
    "npm-run-all": "~4.1.5",
    "raw-loader": "~4.0.2",
    "sass": "~1.56.1",
    "sass-loader": "~13.2.0",
    "tosource": "~2.0.0-alpha.3",
    "ts-jest": "~27.0.4",
    "typescript": "~4.9.4",
    "vite": "~3.2.5",
    "vitest": "0.25.4",
    "vue-tsc": "~1.0.22"
  }
}
