{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.json",
    "type-check:ci": "vue-tsc -p tsconfig.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "build-debug": "cross-env NODE_ENV=debug vite build --mode debug",
    "build-production": "cross-env NODE_ENV=production vite build --mode production",
    "build-development": "cross-env NODE_ENV=development vite build --mode development",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color",
    "lint:silent": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color",
    "test:ci": "vitest --root src/ --run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~6.5.1",
    "@fortawesome/free-solid-svg-icons": "~6.5.1",
    "@fortawesome/vue-fontawesome": "~3.0.5",
    "@vue/runtime-core": "~3.4.15",
    "color": "~4.2.3",
    "lodash": "~4.17.21",
    "mitt": "~3.0.1",
    "sprintf-js": "~1.1.3",
    "vue": "~3.4.15",
    "vuedraggable": "~4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.7.0",
    "@types/color": "~3.0.6",
    "@types/js-yaml": "~4.0.9",
    "@types/jsdom": "~21.1.6",
    "@types/lodash": "~4.14.202",
    "@types/node": "~20.11.5",
    "@types/sprintf-js": "~1.1.4",
    "@typescript-eslint/eslint-plugin": "~6.19.0",
    "@typescript-eslint/parser": "~6.19.0",
    "@vitejs/plugin-vue": "~5.0.3",
    "@vue/compiler-sfc": "~3.4.15",
    "@vue/eslint-config-typescript": "~12.0.0",
    "@vue/reactivity": "~3.4.15",
    "@vue/test-utils": "~2.4.3",
    "@vue/tsconfig": "~0.5.1",
    "cross-env": "~7.0.3",
    "eslint": "~8.56.0",
    "eslint-plugin-vue": "~9.20.1",
    "js-yaml": "~4.1.0",
    "jsdom": "18.0.0",
    "npm": "~10.3.0",
    "npm-run-all": "~4.1.5",
    "sass": "~1.70.0",
    "tosource": "~2.0.0-alpha.3",
    "typescript": "~5.3.3",
    "vite": "~5.0.12",
    "vitest": "0.34.3",
    "vue-tsc": "~1.8.27"
  }
}
