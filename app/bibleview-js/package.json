{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.json",
    "type-check:ci": "vue-tsc -p tsconfig.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "build-debug": "cross-env NODE_ENV=debug vite build --mode debug",
    "build-production": "cross-env NODE_ENV=production vite build --mode production",
    "build-development": "cross-env NODE_ENV=development vite build --mode development",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color",
    "lint:silent": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --no-color",
    "test:ci": "vitest --root src/ --run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~6.4.2",
    "@fortawesome/free-solid-svg-icons": "~6.4.2",
    "@fortawesome/vue-fontawesome": "~3.0.3",
    "@vue/runtime-core": "~3.3.4",
    "color": "~4.2.3",
    "lodash": "~4.17.21",
    "mitt": "~3.0.1",
    "sprintf-js": "~1.1.2",
    "vue": "~3.3.4",
    "vuedraggable": "~4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.3.3",
    "@types/color": "~3.0.3",
    "@types/js-yaml": "~4.0.5",
    "@types/jsdom": "~20.0.1",
    "@types/lodash": "~4.14.197",
    "@types/node": "~18.15.3",
    "@types/sprintf-js": "~1.1.2",
    "@typescript-eslint/eslint-plugin": "~6.5.0",
    "@typescript-eslint/parser": "~6.5.0",
    "@vitejs/plugin-vue": "~4.3.4",
    "@vue/compiler-sfc": "~3.3.4",
    "@vue/eslint-config-typescript": "~11.0.3",
    "@vue/reactivity": "~3.3.4",
    "@vue/test-utils": "~2.3.2",
    "@vue/tsconfig": "~0.4.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.48.0",
    "eslint-plugin-vue": "~9.17.0",
    "js-yaml": "~4.1.0",
    "jsdom": "18.0.0",
    "npm": "~10.0.0",
    "npm-run-all": "~4.1.5",
    "sass": "~1.66.1",
    "tosource": "~2.0.0-alpha.3",
    "typescript": "~5.2.2",
    "vite": "~4.4.9",
    "vitest": "0.34.3",
    "vue-tsc": "~1.8.8"
  }
}
