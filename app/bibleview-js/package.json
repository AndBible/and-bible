{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8085",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "andbible-build-old": "vue-cli-service build --target app --formats umd-min --inline-vue --name BibleView src/main.js",
    "build-debug": "cross-env NODE_ENV=debug vue-cli-service build --target app --inline-vue",
    "build-development": "cross-env NODE_ENV=development vue-cli-service build --target app --inline-vue",
    "build-old": "vue-cli-service build",
    "build-production": "cross-env NODE_ENV=production vue-cli-service build --target app --inline-vue"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "color": "~3.1.3",
    "core-js": "~3.6.5",
    "lodash": "~4.17.20",
    "mitt": "^2.1.0",
    "sprintf-js": "~1.1.2",
    "vue": "^3.0.5",
    "vuedraggable": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.0",
    "babel-eslint": "~10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "~6.7.2",
    "eslint-plugin-vue": "~7.1.0-0",
    "json-loader": "^0.5.7",
    "npm": "^7.6.3",
    "sass": "~1.29.0",
    "sass-loader": "^10.1.1",
    "stylus": "~0.54.8",
    "stylus-loader": "^4.3.3",
    "typescript": "^3.9.9",
    "vue-cli-plugin-webpack-bundle-analyzer": "^4.0.0",
    "vue-cli-plugin-yaml-loader": "~1.0.0",
    "vue-jest": "^5.0.0-alpha.8",
    "yaml-loader": "^0.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "last 10 Android version"
  ]
}
